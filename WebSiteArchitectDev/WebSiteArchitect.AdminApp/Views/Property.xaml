<Window x:Class="WebSiteArchitect.AdminApp.Views.Property"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebSiteArchitect.AdminApp.Views"
        xmlns:my="clr-namespace:WebSiteArchitect.AdminApp.Controls"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Property" Height="600" Width="300">
    <ScrollViewer VerticalScrollBarVisibility="Auto">
        <StackPanel>
            <my:ImageButton Text="Input"  Height="35"/>
            <my:ImageButton Text="Label"  Height="35"/>
            <my:ImageButton Text="Panel"  Height="35"/>
            <my:ImageButton Text="Select"  Height="35"/>
        </StackPanel>
    </ScrollViewer>
</Window>
